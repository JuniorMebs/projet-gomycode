ALGORITHM Tri_insertion
VAR
    Tab : array_name : ARRAY_OF INTEGER[1..20];
    PROCEDURE Tri_insertion(VAR tableau : Tab);
    VAR
        i, j, start : INTEGER;
    BEGIN
        FOR i FROM 2 TO 20  DO
BEGIN
    start:= tableau[i]; j := i-1;
    WHILE (j > 0) && (tableau[j] > start) DO
   BEGIN
        tableau[J+ 1] := tableau[j];
        j := j - 1
    END;
    tableau[j + 1] := start
END ;
END